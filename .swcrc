{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "decorators": true
    },
    "baseUrl": "./src",
    "keepClassNames": true,
    "paths": {
      "#work/*": ["./work/*"],
      "#seedwork/*": ["./@seedwork/*"],
      "#equipment/*": ["./equipment/*"],
      "#quotation/*": ["./quotation/*"],
      "#service-order/*": ["./service-order/*"]
    }
  }
}
